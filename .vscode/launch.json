{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Run GrossToNet",
          "type": "coreclr",
          "request": "launch",
          "preLaunchTask": "build:gross-to-net",
          "program": "${workspaceFolder}/gross-to-net/bin/Debug/net8.0/GrossToNet.dll",
          "args": [],
          "cwd": "${workspaceFolder}",
          "stopAtEntry": false,
          "serverReadyAction": {
              "action": "openExternally",
              "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
          },
          "env": {
              "ASPNETCORE_ENVIRONMENT": "Development"
          },
      }
  ]
}